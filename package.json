{
  "name": "brazilian-values",
  "version": "0.0.0",
  "description": "Validates and formats brazilian values, like money (BRL), CPF, CNPJ, dates etc.",
  "keywords": [
    "brazilian",
    "formatters",
    "validators",
    "RG",
    "CPF",
    "CNPJ",
    "real",
    "BRL"
  ],
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Vitor Luiz Cavalcanti",
    "url": "https://vitorluizc.github.io/",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "main": "./dist/brazilian-values.common.js",
  "module": "./dist/brazilian-values.es.js",
  "watch": {
    "build": "src/*.js",
    "test": "{src,test}/*.js"
  },
  "scripts": {
    "start": "npm-watch",
    "test": "ava ./test/*.test.js",
    "build": "bili ./src/index.js --format es --format cjs --exports named",
    "prepare": "npm run build && npm run test"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "bili": "^0.19.0",
    "moment": "^2.19.2",
    "npm-watch": "^0.3.0"
  },
  "peerDependencies": {
    "moment": "2.x"
  },
  "dependencies": {
    "normalize-text": "^0.2.0"
  }
}
